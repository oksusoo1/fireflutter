{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test:test": "mocha --timeout 5000 --watch tests/test.js",
    "test:messaging": "mocha --timeout 50000 --watch tests/messaging.test.js",
    "test:getTokens": "mocha --timeout 50000 --watch tests/retrieveTokens.test.js",
    "test:meilisearch": "mocha --timeout 5000 --watch tests/meilisearch.test.js",
    "test:backup": "mocha --timeout 5000 --watch tests/backup.test.js",
    "test:messaging:sendMessage": "mocha --timeout 50000 --watch tests/sendPushNotification.test.js",
    "test:uploadFileParentId": "mocha --timeout 10000 --watch tests/uploadFileParentId.test.js",
    "test:getTimestamp": "mocha --timeout 10000 --watch tests/getTimestamp.test.js",
    "lint:fix": "./node_modules/.bin/eslint . --fix"
  },
  "engines": {
    "node": "16"
  },
  "main": "index.js",
  "dependencies": {
    "axios": "^0.26.0",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.18.1",
    "meilisearch": "^0.24.0"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^0.2.0",
    "mocha": "^9.2.0"
  },
  "private": true
}
